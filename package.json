{
  "name": "mybettingslips",
  "version": "1.2.0",
  "description": "Add & Track your betslips, get overview of your record.",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --env=prod && npm run inline",
    "commit": "git add . && git cz && git push",
    "cypress": "cypress open",
    "cypress:chrome": "cypress run --browser chrome",
    "cypress:run": "cypress run",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "prebuild": "rimraf dist",
    "predeploy": "npm run build",
    "deploy": "node ./scripts/deploy",
    "inline": "node ./scripts/inlineCss",
    "lint": "eslint ./src --ext .js --ext .jsx --ext .graphql",
    "stylelint": "stylelint src/components/**/*.scss src/styles/**/*.scss",
    "start": "cross-env NODE_ENV=development webpack-serve",
    "precommit": "lint-staged",
    "prettier": "prettier --config ./.prettierrc --write '{src,graphcool,build-utils,cypress}/**/*.{js,jsx}'",
    "release": "standard-version && git push --follow-tags",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:master": "jest --changedSince=master",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "jest --coverage && ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:size": "bundlesize"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "@sfitzpatrick/enzyme-context-helpers": "^0.2.3",
    "autoprefixer": "^9.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "bundlesize": "^0.17.0",
    "cheerio": "^1.0.0-rc.2",
    "connect-history-api-fallback": "^1.5.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cypress": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "error-overlay-webpack-plugin": "^0.1.5",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-codebox": "^2.0.2",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.0.0",
    "jest-transform-graphql": "^2.1.0",
    "jsonwebtoken": "^8.3.0",
    "kleur": "^2.0.1",
    "koa-connect": "^2.0.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.3.4",
    "react-loadable": "^5.5.0",
    "react-perf-devtool": "^3.0.8",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "speed-measure-webpack-plugin": "^1.2.2",
    "standard-version": "^4.4.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.2",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "dependencies": {
    "@sfitzpatrick/fitzy": "^4.6.21",
    "apollo-cache": "^1.1.14",
    "apollo-cache-inmemory": "1.2.7",
    "apollo-client": "^2.3.8",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.16",
    "formik": "^0.11.11",
    "fractional": "^1.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jest-watch-typeahead": "^0.2.0",
    "jwt-decode": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "styled-components": "^3.4.2",
    "what-input": "^5.1.1",
    "yup": "^0.26.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenfitzpatrick/mybettingslips.git"
  },
  "lint-staged": {
    "src/**/?(*)(Test).js": [
      "jest"
    ],
    "*.{js,jsx}": [
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/vendors~main.bundle.*.js",
      "maxSize": "160 kB"
    },
    {
      "path": "./dist/index.html",
      "maxSize": "6.25 kB"
    }
  ],
  "keywords": [],
  "author": "Steven Fitzpatrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevenfitzpatrick/mybettingslips/issues"
  },
  "homepage": "https://github.com/stevenfitzpatrick/mybettingslips#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
