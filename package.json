{
  "name": "mybettingslips",
  "version": "1.1.1",
  "description": "Add & Track your betslips, get overview of your record.",
  "main": "index.js",
  "scripts": {
    "commit": "git add . && git cz && git push",
    "cypress": "cypress open",
    "cypress:chrome": "cypress run --browser chrome",
    "cypress:run": "wait-on http://localhost:9000 && cypress run",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --env=prod",
    "lint": "eslint {src,cypress,graphcool} --ext .js --ext .jsx",
    "stylelint": "stylelint src/components/**/*.scss src/styles/**/*.scss",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --progress --env=dev",
    "precommit": "lint-staged",
    "prettier": "prettier --config ./.prettierrc --write '{src,graphcool,build-utils,cypress}/**/*.{js,jsx}'",
    "release": "standard-version && git push --follow-tags",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "jest --coverage && ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:size": "bundlesize"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "bundlesize": "^0.16.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "error-overlay-webpack-plugin": "^0.1.4",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-codebox": "^2.0.2",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-graphql": "^1.5.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "3.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "jsonwebtoken": "^8.2.0",
    "lint-staged": "^7.0.0",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.7.2",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-url": "^7.3.1",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "react-hot-loader": "^4.0.0",
    "react-perf-devtool": "^3.0.5",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "speed-measure-webpack-plugin": "^1.2.0",
    "standard-version": "^4.3.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "wait-on": "^2.1.0",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "workbox-webpack-plugin": "^3.0.1"
  },
  "dependencies": {
    "@sfitzpatrick/fitzy": "^4.5.2",
    "apollo-cache": "^1.1.7",
    "apollo-cache-inmemory": "1.1.11",
    "apollo-client": "^2.2.8",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-state": "^0.4.1",
    "classnames": "^2.2.5",
    "formik": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "jwt-decode": "^2.2.0",
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-apollo": "^2.1.2",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.0",
    "styled-components": "^3.2.5",
    "yup": "^0.24.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenfitzpatrick/mybettingslips.git"
  },
  "lint-staged": {
    "src/**/__tests__/*.js": [
      "jest"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "bundlesize": [
    {
      "path": "./dist/main.*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/main.*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/vendors~main.async.*.js",
      "maxSize": "140 kB"
    },
    {
      "path": "./dist/index.html",
      "maxSize": "6.25 kB"
    }
  ],
  "keywords": [],
  "author": "Steven Fitzpatrick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevenfitzpatrick/mybettingslips/issues"
  },
  "homepage": "https://github.com/stevenfitzpatrick/mybettingslips#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
