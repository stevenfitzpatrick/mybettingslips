    "stylelint-order": "^2.0.0",
    "webpack": "^4.29.4",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-deep-scope-plugin": "^1.6.0",
    "webpack-merge": "^4.2.1",
    "webpack-serve": "^2.0.3",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@sfitzpatrick/fitzy": "^4.7.7",
    "apollo-cache-inmemory": "1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "apollo-link-state": "^0.4.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.6",
    "formik": "^1.5.0",
    "fractional": "^1.0.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "jest-watch-typeahead": "^0.2.1",
    "jwt-decode": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.3",
    "styled-components": "^3.4.9",
    "what-input": "^5.1.3",
    "yup": "^0.26.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenfitzpatrick/mybettingslips.git"
  },
  "lint-staged": {
    "src/**/?(*)(Test).js": [
      "jest"
    ],
    "*.{js,jsx}": [
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/vendors~main.bundle.*.js",
      "maxSize": "160 kB"
    },
    {
      "path": "./dist/index.html",
      "maxSize": "6.25 kB"
    }
  ],
